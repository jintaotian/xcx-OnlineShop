/*优惠劵的选择*/

.orderdetail {
  width: 100%;
  height: auto;
}
page{
  background:#fff;
}
.coupon-choose {
  width: 100%;
  height: 200rpx;
  margin: 20rpx 0rpx;
  background-color: #fff;
}

.coupons-con {
  width: 100%;
  float: left;
  height: 100%;
}

.block {
  height: 100%;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}

.coupons {
  width:600rpx;
  height: 200rpx;
  margin-top:30rpx;
  background: url('data:image/png;base64,') no-repeat center center;
  background-size: 600rpx 200rpx;
  text-align: left;
  color: #e4383b;
  float: left;
  overflow: hidden;
  margin-left: 85rpx;
  position:relative;
}

.discount {
  width: 260rpx;
  height: 100%;
  float: left;
  text-align: center;
}

.discount text {
  font-size: 36rpx;
  font-weight: 700;
  color: #fff;
}

.discount .quota {
  font-size: 120rpx;
  font-weight: 700;
  color: #fff;
  line-height: 200rpx;
}
.draw{
    width:92rpx;
    border-left:1rpx dashed #fff;
    height:100%;
    position:absolute;
    top:0rpx;
    right:0rpx;
}
.term {
  width: 235rpx;
  height: 200rpx;
  float: left;
  text-align: left;
}

.term .date {
  font-size: 30rpx;
  color: #fff;
  display: block;
  width: 100%;
  height: 72rpx;
  line-height: 66rpx;
}

.term .full {
  font-size: 30rpx;
  color: #e50338;
  display: block;
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  background: #fff362;
  font-weight: 600;
  text-align:center;
  margin-top:10rpx;
}

.term .time {
  font-size: 30rpx;
  color: #fff;
  display: block;
  width: 100%;
  font-weight: 600;
  height:50rpx;
}

.coupons .requirement {
  display: block;
  font-size: 22rpx;
  color: #e05d32;
}
