/* pages/index/index.wxss */

/* 轮播 */

.banner {
  width: 100%;
  height: 300rpx;
  position: relative;
}

.banner image {
  width: 100%;
  height: 100%;
}

/* banner图dots*/

.banner .wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: 20rpx;
}
.banner .wx-swiper-dot {
  display: inline-flex;
  width: 20rpx;
  height: 20rpx;
  margin-left: 40rpx;
  z-index: 50;
  background: #fff;
  border: 1px solid #2a415c;
}

.banner .wx-swiper-dot-active {
  background: #2a415c;
}

/*优惠劵的选择*/

.orderdetail {
  width: 100%;
  height: auto;
}
.min{
  padding-right:10rpx;
}
.coupon-choose {
  width: 100%;
  height: 200rpx;
  margin: 20rpx 0rpx;
  background-color: #fff;
}

.coupons-con {
  width: 100%;
  float: left;
  height: 100%;
}

.block {
  height: 100%;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}

.coupons {
  width: 450rpx;
  height: 165rpx;
  margin-top:15rpx;
  background: url('data:image/png;base64,') no-repeat center center;
  background-size: 450rpx 165rpx;
  text-align: left;
  color: #e4383b;
  float: left;
  overflow: hidden;
  margin-left: 20rpx;
  position:relative;
}

.discount {
  width: 200rpx;
  height: 100%;
  float: left;
  text-align: center;
}

.discount text {
  font-size: 36rpx;
  font-weight: 700;
  color: #fff;
}

.discount .quota {
  font-size: 110rpx;
  font-weight: 700;
  color: #fff;
  line-height: 150rpx;
}
.draw{
    width:66rpx;
    height:100%;
    position:absolute;
    top:0rpx;
    right:0rpx;
}
.term {
  width: 166rpx;
  height: 150rpx;
  float: left;
  text-align: left;
}

.term .date {
  font-size: 22rpx;
  color: #fff;
  display: block;
  width: 100%;
  height: 50rpx;
  line-height: 60rpx;
}

.term .full {
  font-size: 30rpx;
  color: #e50338;
  display: block;
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  background: #fff362;
  font-weight: 600;
  text-align:center;
  margin-top:10rpx;
}

.term .time {
  font-size: 30rpx;
  color: #fff;
  display: block;
  width: 100%;
  font-weight: 600;
}

.coupons .requirement {
  display: block;
  font-size: 22rpx;
  color: #e05d32;
}



/* 搜索框*/

.none {
  width: 100%;
  height: 20rpx;
  margin: 0 auto;
}

.container .search {
  width: 100%;
  height: 110rpx;
  margin-top: 20rpx;
  background: #fff;
  position: relative;
}

.container .search .Magnifier {
  width: 45rpx;
  height: 45rpx;
  background: url('data:image/png;base64,') no-repeat 0rpx 0rpx;
  background-size: 35rpx 35rpx;
  position: absolute;
  top: 34rpx;
  left: 218rpx;
  z-index: 20;
}

.container .search .Magnifier-box {
  width: 88%;
  height: auto;
  margin: 0 auto;
  background: #ededed;
  border-radius: 8rpx;
}

.container .search .input {
  width: 100%;
  height: 70rpx;
  text-align: center;
}

.container .search .input-search {
  width: 94%;
  height: 70rpx;
  margin-left: 11%;
  line-height: 70rpx;
  color: #666;
}

.container .search .Magnifier-search {
  width: 45rpx;
  height: 70rpx;
  background: url('data:image/png;base64,') no-repeat 0rpx 20rpx;
  background-size: 35rpx 35rpx;
  position: absolute;
  top: 20rpx;
  left: 65rpx;
  z-index: 20;
  background-color: #ededed;
}

/* 商品列表*/

.container .shoplist {
  width: 100%;
  height: 240rpx;
  background: #fff;
  margin-top: 10rpx;
  display: flex;
  position: relative;
}

.container .shoplist .img {
  height: 100%;
  float: left;
  flex: 2;
}

.container .shoplist image {
  width: 80%;
  height: 80%;
  margin-left: 10%;
  margin-top: 10%;
}

.container .shoplist .shopdetail {
  height: 100%;
  float: left;
  flex: 4;
  overflow: hidden;
}

.container .shoplist .shopdetail .shopname {
  font-size: 35rpx;
  margin-top: 50rpx;
  color: #333;
  height: 105rpx;
  overflow: hidden;
  line-height: 50rpx;
}

.container .shoplist .shopdetail .shopprice {
  font-size: 28rpx;
  color: #e4383b;
  position: absolute;
  top: 170rpx;
  left: 245rpx;
}

.container .shoplist .shopdetail .shopprice .price {
  font-size: 40rpx;
  color: #e4383b;
  font-weight: 700;
}

/*没有授权*/

.noPosition {
  padding: 30rpx;
  padding-top: 600rpx;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  margin: auto;
  background: url(data:image/png;base64,) no-repeat center 200rpx;
  background-size: 245rpx 245rpx;
  text-align: center;
  overflow: hidden;
}

.noPosition text {
  display: inline-block;
  line-height: 80rpx;
  overflow: hidden;
  font-size: 32rpx;
  margin-right: 20rpx;
  color: #333;
}

.noPosition button {
  display: inline-block;
  color: #fff;
  overflow: hidden;
  font-size: 32rpx;
  background: linear-gradient(#38ae3b, #226d25);
  box-shadow: 2rpx 2rpx 6rpx #154216;
}

/*加载中*/

.load {
  width: 100%;
  height: 150rpx;
  background-color: #f5f5f5;
  line-height: 150rpx;
  text-align: center;
  position: relative;
}

.load .change {
  width: 100rpx;
  height: 20rpx;
  position: absolute;
  top: 70rpx;
  right: 220rpx;
}

.load .change view {
  width: 10rpx;
  height: 10rpx;
  border-radius: 50% 50%;
  background-color: #fff;
  border: 1rpx solid #333;
  float: left;
  margin-left: 15rpx;
}

.load .loadstyle {
  width: 275rpx;
  height: auto;
  margin: 0 auto;
}

.load .change .on {
  background: #333;
}

.load .loadover {
  color: red;
}

.load .loadover view {
  width: 150rpx;
  height: 1rpx;
  background: #e4383b;
}

.load .loadover .left {
  position: absolute;
  top: 75rpx;
  left: 125rpx;
}

.load .loadover .right {
  position: absolute;
  top: 75rpx;
  right: 125rpx;
}
